;; -*- mode: emacs-lisp; coding: utf-8-unix -*-
;; Minibuffer history file, automatically generated by `savehist'.

(setq savehist-minibuffer-history-variables '(yes-or-no-p-history extended-command-history))
(setq yes-or-no-p-history '("yes"))
(setq extended-command-history '("session-save" "eval-region" "desktop-save-mode" "savehist-mode" "eval-region" "ibuffer" "shell" "indent-region" "eval-region" "tabbar-mode" "ecb-activate"))
